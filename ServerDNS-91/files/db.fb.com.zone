$ORIGIN 	fb.com.
$TTL 		3600
@ 	IN 	SOA 	dns1.fb.com. root.dns1.fb.com (
				1 	; Serial
				3600	; Refresh (1 hour)
				1800	; Retry (30 min)
				604800	; Expire (7 days)
				3600 )	; minimum (1 hour)
@	IN	NS	dns1
router	IN	A 	192.168.91.1
dns1	IN	A	192.168.91.2
ap	IN	A 	192.168.91.3
snmp	IN	A 	192.168.91.4
asterisk	IN	A 	192.168.91.5
owncloud	IN	A	192.168.91.6
radius	IN	A 	192.168.91.7
ldap 	IN	A 	192.168.91.8
www	IN	A	192.168.91.2

